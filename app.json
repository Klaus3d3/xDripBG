{
  "configVersion": "v3",
  "app": {
    "appId": 1003019,
    "appName": "xDripBG",
    "appType": "app",
    "version": {
      "code": 3,
      "name": "1.0.1"
    },
    "icon": "icon.png",
    "vender": "Klaus3d3",
    "description": "Background Services to receive Glucose Data"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.bg_service",   
    "device:os.ble",
    "device:os.local_storage"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    }
  },
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": ["page/index"]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "app-service": {
          "services": ["app-service/bg_service"]
        },
        
        "setting": {
          "path": "setting/index"
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "xDripBG"
    }
  },
  "defaultLanguage": "en-US",
  "debug": true
}
