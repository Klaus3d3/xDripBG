{
  "configVersion": "v3",
  "app": {
    "appId": 1003019,
    "appName": "xDripBG",
    "appType": "app",
    "version": {
      "code": 3,
      "name": "1.0.1"
    },
    "icon": "icon.png",
    "vender": "Klaus3d3",
    "description": "Background Services to receive Glucose Data"
  },
  "permissions": [
    "event:os.customize.fetchsgvdata",
    "event:os.bp.high",
    "event:os.bp.low",
    "event:os.bp.expires",
    "device:os.notification",
    "event:os.health.sleep_status",
    "event:os.health.heart_rate_abnl",
    "event:os.weather.sun_rise",
    "event:os.weather.sun_set",
    "event:os.system.power_saving",
    "event:os.system.theater_mode",
    "event:os.system.no_disturb",
    "event:customize.keyboard",
    "data:os.device.info",
    "device:os.alarm",
    "device:os.accelerometer",
    "device:os.geolocation",
    "device:os.gyroscope",
    "device:os.compass",
    "data:user.hd.sleep",
    "data:user.hd.heart_rate",
    "data:user.hd.spo2",
    "data:user.hd.stress",
    "device:os.bg_service",
    "data:user.hd.workout",
    "device:os.ble",
    "device:os.local_storage"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    }
  },
  "targets": {
    "common": {
      "module": {
        "page": {
          "pages": ["page/index"]
        },
        "app-side": {
          "path": "app-side/index"
        },
        "app-service": {
          "services": ["app-service/bg_service", "app-event/wf_service"]
        },
        "app-event": {
          "path": "app-event/wf_service"
        },
        "setting": {
          "path": "setting/index"
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "xDripBG"
    }
  },
  "defaultLanguage": "en-US",
  "debug": true
}
